set hls
set is
syntax on
set backspace=indent,eol,start
set encoding=utf-8
set fileencodings=utf-8,cp936,gb18030,big5,latin1
filetype plugin indent on
set ambiwidth=double
set spell

" Tab , Indent related
set ts=4
set sw=4
set smarttab
set cin
set ai
set nocp
" Language
set langmenu=zh_CN.UTF-8

" file type check
filetype plugin indent on

